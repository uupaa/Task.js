{
  "name":           "uupaa.task.js",
  "version":        "0.8.21",
  "description":    "Counter based task executor.",
  "url":            "https://github.com/uupaa/Task.js",
  "keywords":       ["async", "task", "flow", "util", "uupaa"],
  "repository": {
    "type":         "git",
    "url":          "https://github.com/uupaa/Task.js.git"
  },
  "scripts": {
    "watch":        "node node_modules/uupaa.watch.js --verbose --action build ./lib/Task.js",
    "build":        "node node_modules/uupaa.minify.js --output ./lib/Task.min.js --verbose --keep --pretty ./lib/Task.js",
    "build-simple": "node node_modules/uupaa.minify.js --output ./lib/Task.min.js --verbose --keep --pretty --nowrap --simple ./lib/Task.js",
    "build-release":"node node_modules/uupaa.minify.js --output ./lib/Task.min.js --verbose --build all",
    "test":         "NODE_ENV=production NODE_PATH=lib node --harmony test/index.node.js; open test/index.html",
    "lint":         "node lint/plato.js; open lint/plato/index.html"
  },
  "build": {
    "all": { "lib/Task.js": "" },
    "node": { },
    "worker": { },
    "browser": { }
  },
  "dependencies": {
  },
  "devDependencies": {
    "uupaa.valid.js":   "",
    "uupaa.help.js":    "",
    "uupaa.test.js":    "",
    "uupaa.watch.js":   "",
    "uupaa.minify.js":  ""
  },
  "lib":            "./lib/",
  "main":           "./index.js",
  "author":         "uupaa <uupaa.js@gmail.com>",
  "license":        "MIT",
  "contributors":   []
}
